# Logging
LOG_LEVEL=INFO

# Trino Connection
# Set these to match your external Trino instance
TRINO_HOST=trino
TRINO_PORT=8080
TRINO_USER=trino
TRINO_CATALOG=tpch
TRINO_SCHEMA=sf1
# TRINO_PASSWORD=
# TRINO_HTTP_SCHEME=http
# TRINO_CONN_TIMEOUT=30
# TRINO_MAX_RETRIES=3
# TRINO_RETRY_DELAY=2

# Network name of the Docker network where Trino is running
TRINO_NETWORK=trino-network

# Redis Cache - No need to modify these for Docker deployment
# They're configured to use the Redis service defined in docker-compose.yml
# REDIS_HOST=redis
# REDIS_PORT=6379
# REDIS_DB=0
# REDIS_PASSWORD=
# SCHEMA_CACHE_TTL=3600

# LLM Configuration (OpenAI example)
LLM_PROVIDER=openai
OPENAI_API_KEY=sk-your-openai-api-key-here
