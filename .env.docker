# OPENAI_MODEL_ANALYSIS=gpt-3.5-turbo
# OPENAI_MODEL_CORRECTION=gpt-4-turbo-preview
# OPENAI_MODEL_EXPLANATION=gpt-3.5-turbo
# AGENT_MAX_RETRIES=2

# API Service
API_KEY=test-key 